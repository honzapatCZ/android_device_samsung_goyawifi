# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#TODO: Add 'check' as fs_mgr_flags with data partition.
# Currently we dont have e2fsck compiled. So fs check would failed.

#<src>                                                <mnt_point>       <type>      <mnt_flags and options>                     		<fs_mgr_flags>
/dev/block/platform/sdhci-pxav3.2/by-name/KERNEL      /boot             emmc        defaults   							defaults
/dev/block/platform/sdhci-pxav3.2/by-name/RECOVERY    /recovery         emmc        defaults   							defaults
/dev/block/platform/sdhci-pxav3.2/by-name/SYSTEM      /system           ext4        ro,noatime		                        		wait
/dev/block/platform/sdhci-pxav3.2/by-name/CACHE       /cache            ext4        noatime,nosuid,nodev,errors=panic           		wait,check
/dev/block/platform/sdhci-pxav3.2/by-name/EFS         /efs              ext4        noatime,nosuid,nodev,errors=panic           		wait,check
/dev/block/platform/sdhci-pxav3.2/by-name/USER        /data             ext4        noatime,nosuid,nodev,errors=panic,discard,noauto_da_alloc 	wait,check,encryptable=footer



#/dev/block/mmcblk0p10    /boot               emmc      defaults   									 defaults
#/dev/block/mmcblk0p9     /recovery           emmc      defaults   									 defaults
#/dev/block/mmcblk0p15    /system             ext4      ro,noatime,errors=panic                                                           wait
#/dev/block/mmcblk0p14    /cache              ext4      nosuid,nodev,noatime,noauto_da_alloc,discard,journal_async_commit,errors=panic    wait,check
#/dev/block/mmcblk0p16    /data               ext4      nosuid,nodev,noatime,noauto_da_alloc,discard,journal_async_commit,errors=panic    wait,check,encryptable=footer
#/dev/block/mmcblk0p4     /efs                ext4      nosuid,nodev,noatime,noauto_da_alloc,discard,journal_async_commit,errors=panic    wait,check


/devices/platform/sdhci-pxav3.0/mmc_host/mmc1         /storage/sdcard1  auto       defaults        						voldmanaged=sdcard:auto