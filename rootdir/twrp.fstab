#mount point    fstype          device 							[device2]
/efs            ext4            /dev/block/platform/sdhci-pxav3.2/by-name/EFS		flags=display="EFS";backup=1
/recovery       emmc            /dev/block/platform/sdhci-pxav3.2/by-name/RECOVERY
/kernel         emmc            /dev/block/platform/sdhci-pxav3.2/by-name/KERNEL
/cache          ext4            /dev/block/platform/sdhci-pxav3.2/by-name/CACHE
/system         ext4            /dev/block/platform/sdhci-pxav3.2/by-name/SYSTEM
/data           ext4            /dev/block/platform/sdhci-pxav3.2/by-name/USER		length=-16384
/external_sd	vfat            /dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Micro SDcard";storage;wipeingui;removable

